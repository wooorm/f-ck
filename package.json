{
  "name": "f-ck",
  "version": "1.0.1",
  "description": "Clean-up cuss words",
  "license": "MIT",
  "keywords": [
    "clean",
    "offensive",
    "profane",
    "vulgar",
    "cuss",
    "profanity",
    "obscenity",
    "swearword",
    "word",
    "grawlix"
  ],
  "repository": "https://github.com/wooorm/f-ck",
  "bugs": "https://github.com/wooorm/f-ck/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "repeat-string": "^1.5.4"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "esmangle": "^1.0.0",
    "nyc": "^8.3.1",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "tape": "^4.0.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --bare -s fck > f-ck.js",
    "build-mangle": "esmangle f-ck.js > f-ck.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test.js",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "ignores": [
      "f-ck.js"
    ]
  },
  "remarkConfig": {
    "output": true,
    "presets": "wooorm"
  }
}
